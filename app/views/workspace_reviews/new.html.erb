<div class="container">
  <h1> Kikou </h1>
  <%= form_for [@workspace, @workspace_review] do |f| %>
    <div class="form-group">
      <%= f.label :score %>
      <%= f.text_field :score, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :content %>
      <%= f.text_field :content, class: 'form-control' %>
    </div>
    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
</div>
